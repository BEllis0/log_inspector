{
    "extends": [
        "google",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },

    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "eol-last": "warn",
        "max-len": "warn",
        "indent": ["error", 4]
    }
}
